rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
    	allow read;
      allow write: if request.auth!=null;
    }
    match /videos {
    	allow read;
      allow write: if false;
    }
    match /users/{userId} {
    	allow read;
      allow write: if request.auth.uid == userId;
    }
  }
}
